            }
            $(this).find("a").click(function() {
                if (jsbps01l_c_on != i) {
                    $(this).parent().addClass("on");
                    if (jsbps01l_c_on != -1) {
                        $(this).closest("ul").find("li").eq(jsbps01l_c_on).removeClass("on")
                    }
                    jsbps01l_c_on = i
                }
            })
        })
    } catch (e) {}
